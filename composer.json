	"keywords": ["nette", "nextras", "presenter", "component", "model", "service"],
	"homepage": "https://github.com/stepapo/generator",
	"license": "MIT",
	"authors": [
		{
			"name": "Štěpán Postler"
		}
	],
	"support": {
		"issues": "https://github.com/stepapo/generator/issues"
	},
	"require": {
		"php": ">=8.0",
		"nette/application": "^3.1",
		"nette/php-generator": "^4.0",
		"nette/utils": "^3.1 || ^4.0",
		"nextras/orm": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"Stepapo\\Generator\\": "src/"
		}
